{
  "extends": "eslint:recommended", 
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "script",
    "ecmaFeatures": {
    }
  },
  "rules": {
    "semi": ["warn", "never"],
    "no-trailing-spaces": ["warn"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "eol-last": ["warn"]
  }
}
